<?xml version="1.0" encoding="utf-8"?>
<ScrollView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    style="@style/ScrollViewStyle"
    android:background="@drawable/equalizer"
    tools:context="com.example.android.musical_structure_app.MainActivity">
    <LinearLayout
        android:id="@+id/outerLinear_songs"
        style="@style/outerLinearVerticalStyle">

        <include
            android:id="@+id/songs_screen1"
            layout="@layout/header"/>

        <TextView
            android:id="@+id/songs_info"
            style="@style/TextStyle3"
            android:text="@string/Songs_info"/>

        <include
            android:id="@+id/songs_screen2"
            layout="@layout/songs"/>

    </LinearLayout>
</ScrollView>
